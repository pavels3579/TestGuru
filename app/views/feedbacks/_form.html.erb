<%= form_with model: @feeedback, local: true do |form| %>
  <%= render 'shared/errors', resource: @feeedback %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title, id: :feedback_title %>
  </p>

  <p>
    <%= form.label :body %><br>
    <%= form.text_field :body, id: :feedback_body %>
  </p>

  <p><%= form.submit %></p>

<% end %>
